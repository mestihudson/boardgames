{
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "standard": "^14.3.1"
  },
  "scripts": {
    "jest": "jest",
    "test:unit": "jest",
    "test:unit:watch": "yarn test:unit --watchAll",
    "test:unit:coverage": "yarn test:unit --coverage --collectCoverageFrom=src/main/**/*.{js}",
    "test:ci": "yarn test:unit:coverage --silent --noStackTrace"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
