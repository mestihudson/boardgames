{
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "standard": "^14.3.1"
  },
  "scripts": {
    "jest": "jest",
    "test:unit": "jest",
    "test:lint": "standard --fix",
    "test:lint:staged": "lint-staged",
    "test:unit:watch": "yarn test:unit --watchAll",
    "test:ci": "yarn test:lint:staged && yarn test:unit:coverage",
    "test:unit:coverage": "yarn test:unit --coverage --silent --noStackTrace"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
